<?xml version="1.0" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:mstns="http://www.rsdn.ru/mapping.xsd"
	xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" xmlns="http://www.rsdn.ru/mapping.xsd"
	id="mapping" targetNamespace="http://www.rsdn.ru/mapping.xsd" attributeFormDefault="qualified"
	elementFormDefault="qualified">
	<xs:element name="mapping" msdata:IsDataSet="true" msdata:EnforceConstraints="False">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="type">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="map" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:attribute name="target" form="unqualified" type="xs:string" use="required" />
									<xs:attribute name="source" form="unqualified" type="xs:string" use="required" />
								</xs:complexType>
							</xs:element>
							<xs:element name="field" minOccurs="0" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="value" minOccurs="0" maxOccurs="unbounded" type="mstns:value"></xs:element>
										<xs:element name="null_value" minOccurs="0" maxOccurs="1" type="mstns:null_value"></xs:element>
										<xs:element name="default_value" minOccurs="0" maxOccurs="1" type="mstns:default_value"></xs:element>
									</xs:sequence>
									<xs:attribute name="name" form="unqualified" type="xs:string" use="required" />
									<xs:attribute name="source" form="unqualified" type="xs:string" use="optional" />
									<xs:attribute name="nullable" form="unqualified" type="xs:boolean" use="optional" default="false" />
									<xs:attribute name="trimmable" form="unqualified" type="xs:boolean" use="optional" default="true" />
									<xs:attribute name="inner_type" form="unqualified" type="xs:string" use="optional" />
									<xs:attribute name="ignore" form="unqualified" type="xs:boolean" default="false" use="optional" />
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="name" form="unqualified" type="xs:string" use="required" />
					</xs:complexType>
				</xs:element>
				<xs:element name="value_type">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="value" minOccurs="0" maxOccurs="unbounded" type="mstns:value"></xs:element>
							<xs:element name="null_value" minOccurs="0" maxOccurs="1" type="mstns:null_value"></xs:element>
							<xs:element name="default_value" minOccurs="0" maxOccurs="1" type="mstns:default_value"></xs:element>
						</xs:sequence>
						<xs:attribute name="name" form="unqualified" type="xs:string" use="required" />
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="value">
		<xs:attribute name="target" form="unqualified" type="xs:string" use="required" />
		<xs:attribute name="source" form="unqualified" type="xs:string" use="required" />
		<xs:attribute name="source_type" form="unqualified" type="xs:string" use="required" />
	</xs:complexType>
	<xs:complexType name="null_value">
		<xs:attribute name="target" form="unqualified" type="xs:string" use="required" />
	</xs:complexType>
	<xs:complexType name="default_value">
		<xs:attribute name="target" form="unqualified" type="xs:string" use="required" />
	</xs:complexType>
</xs:schema>