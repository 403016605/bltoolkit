<?xml version="1.0"?>
<configuration>
	<connectionStrings>
		<add name="Northwind"
			connectionString = "Data Source=.;Database=Northwind;Integrated Security=SSPI"
			providerName     = "System.Data.SqlClient"/>
	</connectionStrings>

	<system.web>
		<compilation debug="true" targetFramework="4.0" />
	</system.web>

	<system.webServer>
		<modules runAllManagedModulesForAllRequests="true"/>
	</system.webServer>
</configuration>
